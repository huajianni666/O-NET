net : "train48.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.001
momentum: 0.9
weight_decay: 0.004
# The learning rate policy
lr_policy: "step"
stepsize: 30000
gamma: 0.1
display: 500   #500
max_iter: 600000
snapshot: 5000
snapshot_prefix: "models/ONet"
solver_mode: GPU
