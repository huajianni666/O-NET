net : "train48.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.004
# The learning rate policy
lr_policy: "step"
stepsize: 30000
gamma: 0.8
display: 500   #500
max_iter: 600000
snapshot: 10000
snapshot_prefix: "./models/"
solver_mode: GPU
